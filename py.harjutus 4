hind = 10






#juubel
vanus = "08.06.2006"
aasta = 2022
p,k,a = vanus.split(".")

tema_vanus = aasta-int(a)

tema_vanus = aasta-int(a)/5



#matemaatika
nr1,nr2 = 8,6
tehe = input("Vali tehe(* / + -):")

if tehe=="*":
    vastus = nr1 * nr2
elif tehe=="/":
    vastus = nr1 / nr2

else:
    vastus = "n/a"



print(f"{nr1}{tehe}{nr2}={vastus}")





#kas on ruut
a = int(input("Sisesta külg 1: "))
b = int(input("Sisesta külg 2: "))
if a==b:
    print(f"{a} {b} moodustavad ruudu")
else:
    print(f"{a} ja {b} moodsutavad ristküliku")
