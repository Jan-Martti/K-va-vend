import turtle

#akna seadad
aken = turtle.Screen()
aken.setup(300,300)
aken.title("Ã¼lesanne 2 - J.M.O")

#kujnudi loomine
k1 = turtle.Turtle()
for i in range(5):
    k1.fd(100)
    k1.rt(144)
    
for i in range (5):
    k1.color("orange")
    k1.backward(100)
    k1.rt(120)

turtle.exitonclick()